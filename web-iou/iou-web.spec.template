%post
mkdir -p /tmp/iou
find /tmp/iou -type d -exec chmod 2755 {} \;
find /tmp/iou -type f -exec chmod 0644 {} \;
rm -rf %{prefix}/log %{prefix}/html/downloads %{prefix}/labs /tmp/iou-web-export.db /tmp/iou-web-import.db /var/lib/php/session/*
chown apache:apache %{prefix}/bin
mkdir -p %{prefix}/labs
chown -R apache:apache %{prefix}/labs /tmp/iou
chmod 755 %{prefix}/labs
mkdir -p %{prefix}/data/Export %{prefix}/data/Import %{prefix}/data/Logs %{prefix}/data/Sniffer
chown -R apache:apache %{prefix}/data
chmod 755 %{prefix}/data/Export %{prefix}/data/Import %{prefix}/data/Logs %{prefix}/data/Sniffer

apachectl graceful

grep xml.cisco.com /etc/hosts > /dev/null
if [ $? -ne 0 ]; then
        echo '127.0.0.127 xml.cisco.com' >> /etc/hosts
fi

if [ ! -f %{prefix}/data/database.sdb ]; then
	cp -a %{prefix}/data/template.sdb %{prefix}/data/database.sdb
fi

echo -n "Updating the database: "
php -q %{prefix}/html/update.php
echo "done"
test -e /usr/lib/libcrypto.so.4
if [ $? -ne 0 ]; then
    rm -f /usr/lib/libcrypto.so.4
    if [ -f /usr/lib/libcrypto.so.10 ]; then
        ln -s libcrypto.so.10 /usr/lib/libcrypto.so.4
    then
        echo "File /usr/lib/libcrypto.so.10 not found, IOL won't start!"
        exit 1
    fi
fi
